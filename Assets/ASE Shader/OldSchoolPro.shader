				float fresnelNdotV69 = dot( tex2DNode45.rgb, WorldViewDirection );
				float fresnelNode69 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV69, _FresnalPow ) );
				float4 appendResult50 = (float4(tex2DNode45.r , tex2DNode45.g , tex2DNode45.b , 0.0));
