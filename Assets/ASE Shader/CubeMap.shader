			o.Emission = tex2D( _CubeMap, WorldReflectionVector( i , tex2DNode5.rgb ).xy ).rgb;
			o.Alpha = 1;
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Standard keepalpha fullforwardshadows 

		ENDCG
